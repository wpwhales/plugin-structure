{
  "name": "wpwhales/plugin-structure",
  "version": "1.0.0",
  "description": "Framework for WPWhales Team to develop plugins in a better way",

  "authors": [
    {
      "name": "Hamza Mirza",
      "email": "hamza.tariq.mirza@gmail.com"
    }
  ],
  "keywords": [
    "wordpress",
    "laravel",
    "plugin development",
    "laravel components in wordpress"
  ],
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-session": "*",
    "ext-tokenizer": "*",
    "brick/math": "^0.9.3|^0.10.2|^0.11",
    "doctrine/inflector": "^2.0.5",
    "egulias/email-validator": "^3.2.1|^4.0",
    "fruitcake/php-cors": "^1.2",
    "guzzlehttp/uri-template": "^1.0",
    "nesbot/carbon": "^2.67",
    "psr/container": "^1.1.1|^2.0.1",
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.2",
    "symfony/mime": "^6.2",
    "voku/portable-ascii": "^2.0",
    "nikic/fast-route": "^1.3",
    "symfony/finder": "^6.4",
    "symfony/console": "^6.0.9",
    "nunomaduro/termwind": "^1.13",
    "woocommerce/action-scheduler": "^3.7",
    "ramsey/uuid": "^4.7",
    "monolog/monolog": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "yoast/phpunit-polyfills": "^2.0",
    "mockery/mockery": "^1.5.1",
    "doctrine/dbal": "^3.5.1",
    "fakerphp/faker": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "WPWCore\\": "src/Core/",
      "WPWhales\\": "src/WPWhales/",
      "WPWhales\\Support\\": [
        "src/WPWhales/Macroable/",
        "src/WPWhales/Collections/",
        "src/WPWhales/Conditionable/"
      ]
    },
    "files": [
      "src/Core/helpers.php",
      "src/Core/Support/helpers.php",
      "src/Core/Collections/helpers.php",
      "src/Core/Filesystem/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/phpunit/testcases"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
