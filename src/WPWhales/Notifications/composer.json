{
    "name": "WPWhales/notifications",
    "description": "The WPWhales Notifications package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "WPWhales/broadcasting": "^10.0",
        "WPWhales/bus": "^10.0",
        "WPWhales/collections": "^10.0",
        "WPWhales/container": "^10.0",
        "WPWhales/contracts": "^10.0",
        "WPWhales/filesystem": "^10.0",
        "WPWhales/mail": "^10.0",
        "WPWhales/queue": "^10.0",
        "WPWhales/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WPWhales\\Notifications\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "WPWhales/database": "Required to use the database transport (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
