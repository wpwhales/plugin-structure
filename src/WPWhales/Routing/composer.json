{
    "name": "WPWhales/routing",
    "description": "The WPWhales Routing package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-filter": "*",
        "ext-hash": "*",
        "WPWhales/collections": "^10.0",
        "WPWhales/container": "^10.0",
        "WPWhales/contracts": "^10.0",
        "WPWhales/http": "^10.0",
        "WPWhales/macroable": "^10.0",
        "WPWhales/pipeline": "^10.0",
        "WPWhales/session": "^10.0",
        "WPWhales/support": "^10.0",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.2",
        "symfony/routing": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "WPWhales\\Routing\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "WPWhales/console": "Required to use the make commands (^10.0).",
        "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
        "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
